
#include <stdio.h>

int main() {
    int a[7];
    int n=7, i, j, temp;
    printf("enter the array elements : ");
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for(j=1;j<n;j++){
        temp=a[j];
        i=j-1;
        	
        while(a[i]>temp && i>-1){
            a[i+1]=a[i];
            i=i-1;
            
        }
        a[i+1]=temp;
    }
    

printf("after sorting : ");
for(i=0;i<n; i++){
    printf("%d\t", a[i]);
}
    return 0;
}
