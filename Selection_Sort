
#include <stdio.h>

int main() {
    int a[]={12,45,14,65,18,85};
    int i, j,temp;
    printf("using selection sort \n");
    
    printf("array elements : ");
    for(i=0;i<=5;i++){
        printf("%d\t", a[i]);
    }
    
    
    for(i=0;i<5-1;i++){
        for(j=i+1;j<5;j++){
            if(a[i]>a[j]){
                temp=a[i];
                a[i]=a[j];
                a[j]=temp;
            }
        }
    }
    
    printf("\nafter sorting : ");
    
    for(i=0;i<=5;i++){
        printf("%d\t", a[i]);
    }

    return 0;
}
